listen "/srv/<%= ENV['APP_NAME'] %>/shared/pids/unicorn.socket"

worker_processes <%= `nproc`.chomp %>

working_directory "/srv/<%= ENV['APP_NAME'] %>/current"

stderr_path "/srv/<%= ENV['APP_NAME'] %>/shared/log/unicorn_stderr.log"
stdout_path "/srv/<%= ENV['APP_NAME'] %>/shared/log/unicorn_stdout.log"

pid "/srv/<%= ENV['APP_NAME'] %>/shared/pids/unicorn.pid"
